function loginAction(){
	var username = $('#username').val();
	var password = $('#password').val();
	$.ajax({
		url: "/login",
		type: "post",
		data: {"username": username, "password": password},
		async: false,
		success: onSuccess
	});
}

function onSuccess(data, status){
	if (data.code == 1) {
		$("#route").click();
	}else {
		alert(data.message);
	}
}

